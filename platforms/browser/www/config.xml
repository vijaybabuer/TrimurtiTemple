<?xml version='1.0' encoding='utf-8'?>
<widget id="com.palpostr.palkar" version="1.1.38" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Trimurti Temple Devasthanam</name>
    <description>Staty Connected with more than 1 Million Palkars world Wide.</description>
    <author email="welcome@palpostr.com" href="http://palpostr.com">PalPostr Team</author>
    <access launch-external="yes" origin="whatsapp:*" />
    <access launch-external="yes" origin="mailto:*" />
    <access launch-external="yes" origin="http://www.palpostr.com/*" />
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-statusbar" source="npm" spec="1.0.0" />
    <gap:plugin name="cordova-plugin-dialogs" source="npm" spec="1.3.1" />
    <gap:plugin name="cordova-plugin-vibration" source="npm" spec="2.1.3" />
    <gap:plugin name="cordova-plugin-file" source="npm" spec="4.3.1" />
    <gap:plugin name="cordova-plugin-camera" source="npm" spec="2.4.0" />
    <gap:plugin name="cordova-plugin-device" source="npm" spec="1.1.5" />
    <gap:plugin name="cordova-plugin-image-picker" source="npm" version="1.1.1" />
    <gap:plugin name="cordova-plugin-x-socialsharing" source="npm" version="5.2.1" />
    <icon src="icon.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/res/icon/android/icon-36-ldpi.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/res/icon/android/icon-48-mdpi.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/res/icon/android/icon-72-hdpi.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/res/icon/android/icon-96-xhdpi.png" />
    <icon gap:platform="blackberry" src="www/res/icon/blackberry/icon-80.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="www/res/icon/blackberry/icon-80.png" />
    <icon gap:platform="ios" height="57" src="www/res/icon/ios/Icon.png" width="57" />
    <icon height="57" platform="ios" src="www/res/ios/Icon.png" width="57" />
    <icon height="1024" platform="ios" src="www/res/icon/ios/iTunesArtWork@2x.png" width="1024" />
    <icon height="72" platform="ios" src="www/res/icon/ios/Icon-60@2x.png" width="72" />
    <icon height="144" platform="ios" src="www/res/icon/ios/Icon-60@3x.png" width="144" />
    <icon height="76" platform="ios" src="www/res/icon/ios/Icon-76.png" width="76" />
    <icon height="152" platform="ios" src="www/res/icon/ios/Icon-76@2x.png" width="152" />
    <icon height="167" platform="ios" src="www/res/icon/ios/Icon-83.5@2x.png" width="167" />
    <icon height="40" platform="ios" src="www/res/icon/ios/Icon-Small-40.png" width="40" />
    <icon height="80" platform="ios" src="www/res/icon/ios/Icon-Small-40@2x.png" width="80" />
    <icon height="120" platform="ios" src="www/res/icon/ios/Icon-Small-40@3x.png" width="120" />
    <icon height="50" platform="ios" src="www/res/icon/ios/Icon-Small-50.png" width="50" />
    <icon height="100" platform="ios" src="www/res/icon/ios/Icon-Small-50@2x.png" width="100" />
    <icon height="29" platform="ios" src="www/res/icon/ios/Icon-Small.png" width="29" />
    <icon height="58" platform="ios" src="www/res/icon/ios/Icon-Small@2x.png" width="58" />
    <icon height="58" platform="ios" src="www/res/icon/ios/Icon-Small@3x.png" width="58" />
    <icon height="114" platform="ios" src="www/res/icon/ios/Icon@2x.png" width="114" />
    <icon height="72" platform="ios" src="www/res/icon/ios/Icon-72.png" width="72" />
    <icon height="144" platform="ios" src="www/res/icon/ios/Icon-72@2x.png" width="144" />
    <icon height="198" platform="ios" src="www/res/icon/ios/apple-icon.png" width="198" />
    <icon height="1024" platform="ios" src="www/res/icon/ios/apple-icon-1024.png" width="1024" />
    <icon height="512" platform="ios" src="www/res/icon/ios/Icon-512.png" width="512" />
    <icon platform="webos" src="www/res/icon/webos/icon-64.png" />
    <icon platform="winphone" src="www/res/icon/windows-phone/icon-48.png" />
    <icon gap:role="background" platform="winphone" src="www/res/icon/windows-phone/icon-173-tile.png" />
    <splash src="splash.png" />
    <splash height="480" platform="ios" src="Default.png" width="320" />
    <splash height="960" platform="ios" src="Default@2x.png" width="640" />
    <splash height="1136" platform="ios" src="Default-568h@2x.png" width="640" />
    <splash height="1334" platform="ios" src="Default-667h@2x.png" width="750" />
    <splash height="2208" platform="ios" src="Default-Portrait-736h@3x.png" width="1242" />
    <splash height="1242" platform="ios" src="Default-Landscape-736h@3x.png" width="2208" />
    <splash height="1024" platform="ios" src="Default-Portrait.png" width="768" />
    <splash height="768" platform="ios" src="Default-Landscape.png" width="1024" />
    <splash height="2048" platform="ios" src="Default-Portrait@2x.png" width="1536" />
    <splash height="1536" platform="ios" src="Default-Landscape@2x.png" width="2048" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="whatsapp:*" />
    <gap:config-file overwrite="true" parent="NSCameraUsageDescription" platform="ios">
        <string>Camera Access for capturing Palkar Community Events by user</string>
    </gap:config-file>
    <gap:config-file overwrite="true" parent="NSPhotoLibraryUsageDescription" platform="ios">
        <string>Photo Library Access to upload photos from Palkar community.</string>
    </gap:config-file>
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="iosPersistentFileLocation" value="Library" />
</widget>
